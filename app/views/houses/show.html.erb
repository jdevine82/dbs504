<p id="notice"><%= notice %></p>

 <div class="container">
  <div class="jumbotron">
    <h1> <%= @house.name %></h1>
    <p><%= @house.blurb %> </p>
  </div>

</div>
 
  
  

<div class='row'>
  <div class='col-md-6'>
